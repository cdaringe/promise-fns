  "version": "0.2.0",
  "configurations": [
    {
      "name": "Deno: Test",
      "request": "launch",
      "type": "pwa-node",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "deno",
      "runtimeArgs": ["test", "--unstable", "--inspect", "--allow-all"],
      "attachSimplePort": 9229
    },
    {
      "name": "Deno: Run",
      "request": "launch",
      "type": "pwa-node",
      "program": "main.ts",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "deno",
      "runtimeArgs": ["run", "--unstable", "--inspect", "--allow-all"],
      "attachSimplePort": 9229
    }
  ]
}
